<div id="verModal" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form class="container" id="form-validation" novalidate method="POST" action="">
                <div class="modal-header">
                    <h4 class="modal-title">Ver Administración</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="closeModal()">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="conceptoAdministracion" class='control-label'>Concepto</label>
                            <input name="administracion[concepto]" type="text" class="form-control" id="conceptoAdministracion"
                                placeholder="Concepto" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaCompraAdministracion" class='control-label'>Fecha de Compra</label>
                            <input name="administracion[fechaCompra]" type="date" class="form-control"
                                id="fechaCompraAdministracion" placeholder="Fecha" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaVentaAdministracion" class='control-label'>Fecha de Venta</label>
                            <input name="" type="date" class="form-control"
                                id="fechaVentaAdministracion" value="" readonly>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="cantidadAdministracion" class='control-label'>Cantidad</label>
                            <input name="administracion[cantidad]" type="number" min="0" class="form-control"
                                id="cantidadAdministracion" placeholder="Cantidad" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="unidadAdministracion" class='control-label'>Unidad</label>
                            <input name="administracion[unidad]" type="text" class="form-control input-small"
                                id="unidadAdministracion" placeholder="" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="valorAdministracion" class='control-label'>Valor Unitario</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" step="0.01"
                                    class="form-control" id="valorAdministracion" placeholder="$/Unidad" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="valorMercadoAdministracion" class='control-label'>Valor de Mercado</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" step="0.01"
                                    class="form-control" id="valorMercadoAdministracion" placeholder="$/Unidad" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="vidaUtilAdministracion" class='control-label'>Vida Útil</label>
                            <input name="administracion[vidaUtil]" type="number" min="0" step="1" class="form-control"
                                id="vidaUtilAdministracion" placeholder="Años" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="antiguedadAdministracion" class='control-label'>Antigüedad</label>
                            <input name="" type="number" min="0" step="1" class="form-control"
                                id="antiguedadAdministracion" placeholder="Años" readonly>
                        </div>
                        
                        <div class="form-group col-md-6">
                            <label for="estadoAdministracion" class='control-label'>Estado</label>
                            <input name="administracion[estado]" type="text" class="form-control input-small"
                                id="estadoAdministracion" placeholder="" readonly>
                        </div>
                    </div>
                    
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="amortizacionAdministracion" class='control-label'>Amortización</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01"
                                    class="form-control" id="amortizacionAdministracion" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="amortizacionAcumuladaAdministracion" class='control-label'>Amortización Acumulada</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01"
                                    class="form-control" id="amortizacionAcumuladaAdministracion" value="0" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="valorResidualAdministracion" class='control-label'>Porc. Valor Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">%</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01"
                                    class="form-control" id="valorResidualAdministracion" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="valorResidualMontoAdministracion" class='control-label'>Monto Valor Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01"
                                    class="form-control" id="valorResidualMontoAdministracion" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="valorANuevoAdministracion" class='control-label'>Valor A Nuevo</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01"
                                    class="form-control" id="valorANuevoAdministracion" value="0" readonly>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Cerrar" onclick="closeModal()">
                </div>
            </form>
        </div>
    </div>
</div>