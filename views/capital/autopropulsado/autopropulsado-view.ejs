<div id="verModal" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form class="container" id="form-validation" novalidate method="POST" action="">
                <div class="modal-header">
                    <h4 class="modal-title">Ver Autopropulsado</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"
                        onclick="closeModal()">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="conceptoAutopropulsado" class='control-label'>Concepto</label>
                            <input name="autopropulsado[concepto]" type="text" class="form-control"
                                id="conceptoAutopropulsado" placeholder="Concepto" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="utaAutopropulsado" class='control-label'>UTA</label>
                            <input name="autopropulsado[uta]" type="number" min="0" step="0.01" class="form-control"
                                id="utaAutopropulsado" placeholder="UTA" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaCompraAutopropulsado" class='control-label'>Fecha de Compra</label>
                            <input name="autopropulsado[fechaCompra]" type="date" class="form-control"
                                id="fechaCompraAutopropulsado" placeholder="Fecha" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaVentaAutopropulsado" class='control-label'>Fecha de Venta</label>
                            <input name="" type="date" class="form-control" id="fechaVentaAutopropulsado" value=""
                                readonly>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="potenciaAutopropulsado" class='control-label'>Potencia</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">HP</span>
                                </div>
                                <input name="autopropulsado[potencia]" type="number" min="0" step="0.01"
                                    class="form-control" id="potenciaAutopropulsado" placeholder="Potencia" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="anchoTrabajoAutopropulsado" class='control-label'>Ancho de Trabajo</label>
                            <input name="autopropulsado[anchoTrabajo]" type="number" min="0" step="0.01"
                                class="form-control" id="anchoTrabajoAutopropulsado" placeholder="Ancho de Trabajo"
                                readonly>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="valorAutopropulsado" class='control-label'>Valor Unitario</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" step="0.01" class="form-control"
                                    id="valorAutopropulsado" placeholder="$/Unidad" readonly>
                            </div>
                        </div>

                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="vidaUtilAutopropulsado" class='control-label'>Vida Útil</label>
                            <input name="autopropulsado[vidaUtil]" type="number" min="0" step="1" class="form-control"
                                id="vidaUtilAutopropulsado" placeholder="Años" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="antiguedadAutopropulsado" class='control-label'>Antigüedad</label>
                            <input name="" type="number" min="0" step="1" class="form-control"
                                id="antiguedadAutopropulsado" placeholder="Años" readonly>
                        </div>

                        <div class="form-group col-md-6">
                            <label for="estadoAutopropulsado" class='control-label'>Estado</label>
                            <input name="autopropulsado[estado]" type="text" class="form-control input-small"
                                id="estadoAutopropulsado" placeholder="" readonly>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="amortizacionAutopropulsado" class='control-label'>Amortización</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="amortizacionAutopropulsado" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="amortizacionAcumuladaAutopropulsado" class='control-label'>Amortización
                                Acumulada</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="amortizacionAcumuladaAutopropulsado" value="0" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="valorResidualAutopropulsado" class='control-label'>Porc. Valor Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">%</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorResidualAutopropulsado" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="valorResidualMontoAutopropulsado" class='control-label'>Monto Valor
                                Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorResidualMontoAutopropulsado" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="valorANuevoAutopropulsado" class='control-label'>Valor A Nuevo</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorANuevoAutopropulsado" value="0" readonly>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Cerrar"
                        onclick="closeModal()">
                </div>
            </form>
        </div>
    </div>
</div>