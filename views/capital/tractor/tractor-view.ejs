<div id="verModal" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form class="container" id="form-validation" novalidate method="POST" action="">
                <div class="modal-header">
                    <h4 class="modal-title">Ver Tractor</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"
                        onclick="closeModal()">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="conceptoTractor" class='control-label'>Concepto</label>
                            <input name="tractor[concepto]" type="text" class="form-control" id="conceptoTractor"
                                placeholder="Concepto" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaCompraTractor" class='control-label'>Fecha de Compra</label>
                            <input name="tractor[fechaCompra]" type="date" class="form-control" id="fechaCompraTractor"
                                placeholder="Fecha" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="fechaVentaTractor" class='control-label'>Fecha de Venta</label>
                            <input name="" type="date" class="form-control" id="fechaVentaTractor" value="" readonly>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="potenciaTractor" class='control-label'>Potencia</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">HP</span>
                                </div>
                                <input name="tractor[potencia]" type="number" min="0" step="0.01" class="form-control"
                                    id="potenciaTractor" placeholder="Potencia" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="valorTractor" class='control-label'>Valor Unitario</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" step="0.01" class="form-control" id="valorTractor"
                                    placeholder="$/Unidad" readonly>
                            </div>
                        </div>
                        
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="vidaUtilTractor" class='control-label'>Vida Útil</label>
                            <input name="tractor[vidaUtil]" type="number" min="0" step="1" class="form-control"
                                id="vidaUtilTractor" placeholder="Años" readonly>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="antiguedadTractor" class='control-label'>Antigüedad</label>
                            <input name="" type="number" min="0" step="1" class="form-control" id="antiguedadTractor"
                                placeholder="Años" readonly>
                        </div>

                        <div class="form-group col-md-6">
                            <label for="estadoTractor" class='control-label'>Estado</label>
                            <input name="tractor[estado]" type="text" class="form-control input-small"
                                id="estadoTractor" placeholder="" readonly>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="amortizacionTractor" class='control-label'>Amortización</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="amortizacionTractor" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="amortizacionAcumuladaTractor" class='control-label'>Amortización
                                Acumulada</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="amortizacionAcumuladaTractor" value="0" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label for="valorResidualTractor" class='control-label'>Porc. Valor Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">%</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorResidualTractor" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="valorResidualMontoTractor" class='control-label'>Monto Valor Residual</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorResidualMontoTractor" value="0" readonly>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="valorANuevoTractor" class='control-label'>Valor A Nuevo</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">$</span>
                                </div>
                                <input name="" type="number" min="0" max="100" step="0.01" class="form-control"
                                    id="valorANuevoTractor" value="0" readonly>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <input type="button" class="btn btn-default" data-dismiss="modal" value="Cerrar"
                        onclick="closeModal()">
                </div>
            </form>
        </div>
    </div>
</div>